<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='langtest'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>

<!--
  <inherits name="com.google.gwt.logging.Logging"/>
-->

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <inherits name='com.google.gwt.user.theme.clean.Clean'/>
<!--
  <inherits name='com.googlecode.gchart.GChart'/>
-->
  <inherits name='com.google.gwt.visualization.Visualization'/>
  <inherits name ="com.github.gwtbootstrap.Bootstrap"/>
  <inherits name="org.moxieapps.gwt.highcharts.Highcharts"/>

  <set-property name="bootstrap.responsiveDesign" value="true"/>

  <!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

  <!-- Other module inherits                                      -->

  <!-- Specify the app entry point class.                         -->
  <entry-point class='mitll.langtest.client.LangTest'/>

  <!-- Specify the paths for translatable code                    -->
  <source path='client'/>
  <source path='shared'/>

  <public path='public'/>

  <!-- These 3 scripts are necessary so that Flash Recording can work -->
  <script src="js/jquery-1.10.2.min.js"/>
<!--
  <script src="js/jquery-migrate-1.1.1.min.js"/>
-->
  <script src="js/swfobject.js"/>
  <script src="js/recorder.js"/>

  <script src="js/gauge.js"/>

    <script src="js/highcharts.js"/>

</module>
